   "source": [
    "import pyautogui as auto\n",
    "from pynput.keyboard import Key, Controller\n",
    "\n",
    "keyboard = Controller()\n",
    "\n",
    "auto.position()\n",
    "\n",
    "msg = open('Contagem.txt', 'r')\n",
    "auto.click(auto.position())\n",
    "\n",
    "for x in msg:\n",
    "    auto.write(x)\n",
    "    keyboard.press(Key.enter)\n",
    "    keyboard.release(Key.enter)"
   ]
  }
